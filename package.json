{
  "name": "4so-web-template",
  "version": "1.0.3",
  "main": "index.html",
  "description": "Basic template for new websites.",
  "repository": {
    "type": "git",
    "url": "git@gitlab.4so.local:fourseasons/4so-web-template.git"
  },
  "keywords": [
    "HTML5",
    "CSS",
    "JavaScript",
    "SASS"
  ],
  "bugs": {
    "url": "http://gitlab.4so.local/fourseasons/4so-web-template/issues"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "webpack": "webpack",
    "webpack-watch": "webpack --watch",
    "sass-watch": "node-sass --watch --recursive --output src/css/ --source-map true --source-map-contents src/sass/",
    "sass-compile": "node-sass --output src/css/ --source-map true --source-map-contents src/sass/",
	"sass-compile-prod": "rimraf src/css && node-sass --output src/css/ --output-style compressed src/sass/",
    "svg-sprite": "rimraf src/img/sprite.svg && svg-sprite --config svg-sprite.json src/img/**/*.svg",
    "stylelint": "stylelint \"src/sass/**/*.scss\" || echo"
  },
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "fourseasons",
    "email": "info@4so.de",
    "url": "http://www.4so.de"
  },
  "contributors": [
    "Henryk Kurze",
    "Theo Salzmann"
  ],
  "homepage": "http://gitlab.4so.local/fourseasons/4so-web-template",
  "dependencies": {},
  "engines": {},
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "node-sass": "^4.1.1",
    "rimraf": "^2.5.4",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-scss": "^1.4.1",
    "svg-sprite": "^1.3.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.2"
  }
}
